{
  "name": "azure-functions-swagger-ui",
  "version": "1.0.1",
  "description": "A simple npm module that integrates Swagger UI into Azure Functions, providing a user-friendly interface to visualize and interact with your API endpoints. Perfect for documenting and testing Azure Functions with minimal setup.",
  "keywords": [
    "swagger",
    "azure",
    "swagger-ui",
    "azure-api",
    "api-documentation",
    "openapi",
    "swagger-ui-azure",
    "azure-functions",
    "api-management",
    "rest-api",
    "api-explorer",
    "documentation"
  ],
  "homepage": "https://github.com/Le-Roi-du-village/azure-functions-swagger-ui#readme",
  "bugs": {
    "url": "https://github.com/Le-Roi-du-village/azure-functions-swagger-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Le-Roi-du-village/azure-functions-swagger-ui.git"
  },
  "license": "Apache-2.0",
  "author": "Le-Roi-du-village",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "module": "dist/esm/index.js",
  "main": "dist/cjs/index.js",
  "scripts": {
    "build": "tsc --outDir dist/cjs  && tsc --module es6 --outDir dist/esm",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "@azure/functions": "^4.0.0"
  },
  "dependencies": {
    "swagger-ui-dist": "^5.18.2"
  },
  "files": [
    "dist",
    "swagger-initializer.js"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  }
}
